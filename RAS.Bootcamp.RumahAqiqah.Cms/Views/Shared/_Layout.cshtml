<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewData["Title"] - RAS.Bootcamp.RumahAqiqah.Cms</title>

    <link rel="preconnect" href="https://fonts.gstatic.com">
    <link href="https://fonts.googleapis.com/css2?family=Nunito:wght@300;400;600;700;800&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="~/Template/assets/css/style.css">
    <link rel="stylesheet" href="~/Template/assets/css/bootstrap.css">

    <link rel="stylesheet" href="~/Template/assets/vendors/iconly/bold.css">

    <link rel="stylesheet" href="~/Template/assets/vendors/perfect-scrollbar/perfect-scrollbar.css">
    <link rel="stylesheet" href="~/Template/assets/vendors/bootstrap-icons/bootstrap-icons.css">
    <link rel="stylesheet" href="~/Template/assets/css/app.css">
    <link rel="shortcut icon" href="~/Template/assets/images/logo/LogoNav.png" type="image/x-icon">
</head>

<body>
    <div id="sidebar" class="active">
        <div class="sidebar-wrapper active">
            <div class="sidebar-header">
                <div class="d-flex justify-content-between">
                    <div class="logo">
                        <a href="/Home"><img src="~/Template/assets/images/logo/logo.png" alt="Logo" srcset=""
                                style="width: 100%; height:auto;"></a>
                    </div>
                    <div class="toggler">
                        <a class="sidebar-hide d-xl-none d-block"><i class="bi bi-x bi-middle"></i></a>
                    </div>
                </div>
            </div>
            <div class="sidebar-menu">
                <ul class="menu">
                    <li class="sidebar-title"><b>Menu</b></li>

                    <li class="sidebar-item">
                        <a href="/Home/Index" class='sidebar-link'>
                            <i class="bi bi-grid-fill"></i>
                            <span>Dashboard</span>
                        </a>
                    </li>
                    <li class="sidebar-item">
                        <a href="/Article/Index" class='sidebar-link'>
                            <i class="bi-solid bi-newspaper"></i>
                            <span>Article</span>
                        </a>
                    </li>
                    <li class="sidebar-item  has-sub">
                        <a class='sidebar-link'>
                            <i class="bi bi-stack"></i>
                            <span>Menu Order</span>
                        </a>
                        <ul class="submenu ">
                            <li class="submenu-item ">
                                <a href="/Order/Index">Order</a>
                            </li>
                            <li class="submenu-item ">
                                <a href="/OrderDelivery/Index">Order Delivery</a>
                            </li>
                            <li class="submenu-item ">
                                <a href="/OrderDetail/Index">Order Detail</a>
                            </li>
                            <li class="submenu-item ">
                                <a href="/OrderHistory/Index">Order History</a>
                            </li>

                        </ul>
                    </li>
                    <li class="sidebar-item">
                        <a href="/Product/Index" class='sidebar-link'>
                            <i class="bi-solid bi-newspaper"></i>
                            <span>Product</span>
                        </a>
                    </li>
                    <li class="sidebar-item">
                        <a href="/Account" class='sidebar-link'>
                            <i class="bi-solid bi-newspaper"></i>
                            <span>Account</span>
                        </a>
                    </li>
                    <li class="sidebar-item">
                        <a href="/User" class='sidebar-link'>
                            <i class="bi-solid bi-newspaper"></i>
                            <span>User</span>
                        </a>
                    </li>
                    <li class="sidebar-item">
                        <a href="/UserOrderChild" class='sidebar-link'>
                            <i class="bi-solid bi-newspaper"></i>
                            <span>User Child</span>
                        </a>
                    </li>
                </ul>
            </div>
            <button class="sidebar-toggler btn x"><i data-feather="x"></i></button>
            <a href="" class="btn btn-danger ms-5">Log Out</a>
        </div>
    </div>
    <div id="main">
        <header class="mb-3">
            <a class="burger-btn d-block d-xl-none">
                <i class="bi bi-justify fs-3"></i>
            </a>
        </header>

        <main role="main" class="pb-3">
            @RenderBody()
        </main>


        <footer>
            <div class="footer clearfix mb-0 text-muted">
                <div class="float-end">
                    <p>2022 &copy; <b>Rumah Aqiqah Salman</b></p>
                </div>
            </div>
        </footer>
    </div>
    </div>
    <script src="~/Template/assets/vendors/perfect-scrollbar/perfect-scrollbar.min.js"></script>
    <script src="~/Template/assets/js/bootstrap.bundle.min.js"></script>

    <script src="~/Template/assets/vendors/apexcharts/apexcharts.js"></script>
    <script src="~/Template/assets/js/pages/dashboard.js"></script>

    <script src="~/Template/assets/js/mazer.js"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>

</html>